{
    "name": "zhuravljov/yii2-pagination",
    "description": "Yii2 Pagination Widgets",
    "type": "yii2-extension",
    "keywords": ["yii", "pagination", "widget"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Roman Zhuravlev",
            "email": "zhuravljov@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/zhuravljov/yii2-pagination/issues",
        "source": "https://github.com/zhuravljov/yii2-pagination"
    },
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "~2.0.18"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "zhuravljov\\yii\\pagination\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
